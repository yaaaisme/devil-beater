    local NoCdKeybind = Tabs.Main:AddKeybind("NoCd", {
        Title = "No cooldown",
        Mode = "Toggle", -- Always, Toggle, Hold
        Default = "T", -- String as the name of the keybind (MB1, MB2 for mouse buttons)

        -- Occurs when the keybind is clicked, Value is `true`/`false`
        Callback = function(Value)
        local player = game:GetService("Players").LocalPlayer
        local move = player.PlayerGui.CombatGui.Scale.CooldownFrame.Moves["4"].Cooldown
        if move then
            move.Name = "T"
        end
        end,

        -- Occurs when the keybind itself is changed, `New` is a KeyCode Enum OR a UserInputType Enum
        ChangedCallback = function(New)            
        
        end
    })

    Tabs.Main:AddButton({
        Title = "Run L to autoskip script",
        Description = "no need to click again",
        Callback = function()
            local isHolding = false
-- Detect when the key is pressed
UserInputService.InputBegan:Connect(function(input, isProcessed)
    if isProcessed then return end -- Ignore inputs processed by the game/UI
    if input.KeyCode == Enum.KeyCode.L then
        isHolding = true
        -- Start a loop while holding
        while isHolding do
        local args = {[1] = "SkipCutscene"}
            game:GetService("ReplicatedStorage"):WaitForChild("ActModuleEvent"):FireServer(unpack(args))
            task.wait(0.1) -- Adjust loop speed
        end
    end
end)
-- Detect when the key is released
UserInputService.InputEnded:Connect(function(input, isProcessed)
    if isProcessed then return end
    if input.KeyCode == Enum.KeyCode.L then
        isHolding = false
    end
end)
        end
    })


    Tabs.Main:AddButton({
        Title = "Run H to spam demix script",
        Description = "no need to click again",
        Callback = function()
            local isHolding = false
            -- Detect when the key is pressed
            UserInputService.InputBegan:Connect(function(input, isProcessed)
