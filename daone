    local AdventDropdown = Tabs.Acts:AddDropdown("Dropdown", {
        Title = "Advent select",
        Values = {"Shadow Society", "Mythology", "Heroes of Light", "Rotomo Family Holiday", "Gold Rush", "The Laboratory(unfinished)", "Testing Realm"},
        Multi = false,
        Default = nil,
    })


    AdventDropdown:OnChanged(function(Value)
        if Value == "Shadow Society" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 3124999767, true, 8543541285)
        elseif Value == "Mythology" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 3124999767, true, 6930614828)
        elseif Value == "Heroes of Light" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 3124999767, true, 7169453216)
        elseif Value == "Rotomo Family Holiday" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 3124999767, true, 7550694574)
        elseif Value == "Gold Rush" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 3124999767, true, 14369886144)
        elseif Value == "The Laboratory(unfinished)" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 3124999767, true, 7328473256)
        elseif Value == "Testing Realm" then 
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer("TeleportParty", 8830701703, true)
        end
    end)

