                        local args = {[1] = "Capella",[2] = 528649935}
                        game:GetService("Players").LocalPlayer.Character.Demixeur.RemoteEvent:FireServer(unpack(args))
                        task.wait() -- Adjust loop speed
                    end
                end
            end)
            -- Detect when the key is released
            UserInputService.InputEnded:Connect(function(input, isProcessed)
                if isProcessed then return end
                if input.KeyCode == Enum.KeyCode.H then
                    isHolding = false
                end
            end)
        end
    })

